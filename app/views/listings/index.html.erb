<h1>Listing listings</h1>

<table>
  <tr>
    <th>Searchid</th>
    <th>Freecycgroup</th>
    <th>Freecychood</th>
    <th>Freecyclistid</th>
    <th>Dateposted</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @listings.each do |listing| %>
  <tr>
    <td><%= listing.searchid %></td>
    <td><%= listing.freecycgroup %></td>
    <td><%= listing.freecychood %></td>
    <td><%= listing.freecyclistid %></td>
    <td><%= listing.dateposted %></td>
    <td><%= link_to 'Show', listing %></td>
    <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
    <td><%= link_to 'Destroy', listing, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Listing', new_listing_path %>
